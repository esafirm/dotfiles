#!/bin/bash
#Generated by JetBrains Toolbox 2.1.1.18388 at 2023-12-05T10:48:33.958574

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/esafirm/Applications/IntelliJ IDEA Community Edition.app/Contents/MacOS/idea" $wait --args "${intellij_args[@]}"