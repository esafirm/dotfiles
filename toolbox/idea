#!/bin/bash
#Generated by JetBrains Toolbox 2.3.2.31487 at 2024-06-24T08:56:15.763918

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/esafirm/Applications/IntelliJ IDEA Community Edition.app/Contents/MacOS/idea" $wait --args "${intellij_args[@]}"