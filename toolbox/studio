#!/bin/bash
#Generated by JetBrains Toolbox 2.2.0.19611 at 2024-03-05T12:16:56.073808

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/esafirm/Applications/Android Studio.app/Contents/MacOS/studio" $wait --args "${intellij_args[@]}"