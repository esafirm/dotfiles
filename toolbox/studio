#!/bin/bash
#Generated by JetBrains Toolbox 2.1.0.18144 at 2023-11-17T12:16:31.210253

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/esafirm/Applications/Android Studio.app/Contents/MacOS/studio" $wait --args "${intellij_args[@]}"